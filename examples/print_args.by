func main = argv: List[String] -> Int {
    let list = printlist(argv);
    let list2 =printlist(list);
    0
}

func printlist = list: List[String] -> List[String] {
    let b = list_has_next(list);
    let head : tail = list;
    if b {
        printf("%s, ", head);
        head : printlist(tail)
    } else {
        printf("%s;\n", head);
        head : tail
    }
}

extern func printf = String, String -> Void
extern func list_has_next = List -> Bool